{
	"name": "@jad3n/data-schema",
	"version": "1.0.0",
	"description": "A binary packet library, useful for working with WebSockets for small but structured packets.",
	"main": "dist/data-schema.umd.js",
	"module": "dist/data-schema.es5.js",
	"types": "dist/types/index.d.ts",
	"scripts": {
		"clean": "rimraf ./dist/",
		"build": "npm run clean && rollup -c rollup.config.js",
		"watch": "rollup -w -c rollup.config.js",
		"doc": "typedoc src/ --target es6 --mode file --out ./docs/"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/JAD3N/data-schema.git"
	},
	"author": "Jaden Buchan",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/JAD3N/data-schema/issues"
	},
	"homepage": "https://github.com/JAD3N/data-schema",
	"devDependencies": {
		"rimraf": "^2.6.3",
		"rollup": "^1.16.6",
		"rollup-plugin-commonjs": "^10.0.1",
		"rollup-plugin-json": "^4.0.0",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-sourcemaps": "^0.4.2",
		"rollup-plugin-typescript2": "^0.21.2",
		"typedoc": "^0.14.2",
		"typescript": "^3.5.2"
	}
}
